<?php

/**
 * @file
 * Install/uninstall functions for the viewer.
 */

/**
 * Implements hook_install().
 */
function islandora_ohms_viewer_install() {
  variable_set('islandora_ohms_viewer_link_xml', FALSE);
}

/**
 * Implements hook_uninstall().
 */
function islandora_ohms_viewer_uninstall() {
  variable_del('islandora_ohms_viewer_link_xml');
}
